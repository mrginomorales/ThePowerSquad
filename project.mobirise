con Size\" name=\"IconSize\" min=\"20\" max=\"80\" step=\"5\" value=\"40\" condition=\"showIcon\">\n        <input type=\"color\" title=\"Card Color\" value=\"#eef3f9\" name=\"cardColor\">\n        <select title=\"Align\" name=\"cardsAlign\">\n                <option value=\"1\">Left</option>\n                <option value=\"2\" selected=\"\">Center</option>\n                <option value=\"3\">Right</option>\n            </select>\n         <header condition=\"!fullWidth\">Background</header>   \n             <input type=\"color\" title=\"Background Color\" value=\"#eef3f9\" name=\"bgColor\" condition=\"!fullWidth\">\n    </mbr-parameters>\n\n   <div mbr-class=\"{'container':!fullWidth,\n                     'container-fluid': fullWidth}\">\n        <div class=\"mbr-row\" mbr-class=\"{'mbr-row-reverse' : reverse}\">\n            <div class=\"mbr-col-sm-12 mbr-col-md-6\" mbr-class=\"{'mbr-col-lg-3' : showCard1,  'mbr-col-lg-6' : !showCard1}\">\n                <div class=\"image item1\">\n                    <div class=\"mbr-overlay\"></div>\n                    <amp-img src=\"@PROJECT_PATH@/assets/images/fast-start-training-712x401.jpeg\" alt=\"image\" height=\"300\" width=\"532.6683291770573\" layout=\"responsive\" class=\"mobirise-loader\">\n                        <div placeholder=\"\" class=\"placeholder\"></div>\n                       <img src=\"@PROJECT_PATH@/assets/images/fast-start-training-712x401.jpeg\" class=\"mbr-temp\" alt=\"\" title=\"\">\n                    </amp-img>\n                </div>\n                </div>\n            <div class=\"mbr-col-sm-12 mbr-col-md-6 mbr-flex mbr-jc-c box\" mbr-class=\"{'mbr-col-lg-3' : showCard1,  'mbr-col-lg-6' : !showCard1}\">\n                <div class=\"text__block text1 align-center\" mbr-class=\"{'align-center' : cardsAlign == 2, 'align-right' : cardsAlign == 3}\">\n                    <div mbr-if=\"showIcon\"><span class=\"mbr-iconfont-btn mbr-iconfont icon__text fa-flash fa\" mbr-icon=\"\"></span></div>\n